<table class="packet">
<tr type="{{lineType}}" class="line {{lineClassAttributeRepresentation}}" id="line{{lineNumber}}">
  <td></td>
  <td class="msg">
	<p>
		<span class="time">{{formattedTimestamp}} </span>
		<span class="message" type="{{lineType}}">
			{{{formattedMessage}}}
		</span>
	</p>
	</td>
</tr>
</table>